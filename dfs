#include <stdio.h>
#include <stdlib.h>

#define MAX 100

int visited[MAX];

// DFS function
void dfs(int n, int graph[n][n], int node) {
    visited[node] = 1;
    printf("%d ", node);

    for (int i = 0; i < n; i++) {
        if (graph[node][i] == 1 && !visited[i]) {
            dfs(n, graph, i);
        }
    }
}

int main() {
    int n, start;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    int graph[n][n];

    printf("Enter adjacency matrix:\n");
